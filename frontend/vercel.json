{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://timber-trend-backend-git-main-tush-tecs-projects.vercel.app/api/v1/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://timber-trend.onrender.com" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}
